ePublicKey = () => {\n  if (!window.STRIPE_PUBLIC_KEY) {\n    console.error('Stripe public key is not defined');\n    throw new Error('Payment system is not configured properly');\n  }\n  return window.STRIPE_PUBLIC_KEY;\n};\n\nexport const bookTour = async (tourId) => {\n  try {\n    // Validate tourId\n    if (!tourId || typeof tourId !== 'string') {\n      throw new Error('Invalid tour ID');\n    }\n\n    // Show loading state\n    showAlert('loading', 'Preparing your booking...');\n\n    // Initialize Stripe if not already done\n    if (!stripe) {\n      stripe = Stripe(getStripePublicKey());\n    }\n\n    // 1) Get checkout session from API\n    const { data } = await axios({\n      method: 'GET',\n      url: `/api/v1/bookings/checkout-session/${tourId}`,\n      timeout: 10000 // 10 seconds timeout\n    });\n\n    if (!data?.session?.id) {\n      throw new Error('Invalid session data received');\n    }\n\n    // 2) Redirect to Stripe Checkout\n    const { error } = await stripe.redirectToCheckout({\n      sessionId: data.session.id\n    });\n\n    if (error) {\n      throw error;\n    }\n\n  } catch (err) {\n    console.error('Booking error:', err);\n    \n    // Handle specific error cases\n    let errorMessage = 'There was an error processing your payment. Please try again.';\n    \n    if (err.message.includes('timeout')) {\n      errorMessage = 'Request timed out. Please check your connection and try again.';\n    } else if (err.response?.data?.message) {\n      errorMessage = err.response.data.message;\n    } else if (err.message) {\n      errorMessage = err.message;\n    }\n\n    showAlert('error', errorMessage);\n    \n    // Return the error for additional handling if needed\n    return Promise.reject(err);\n  }\n};"},"lineCount":null}},"error":null,"hash":"1237bcd3c1d2562ad3708d05b656789e","cacheData":{"env":{}}}